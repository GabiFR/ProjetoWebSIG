var pontos_interesse={"type":"FeatureCollection","features":[{"type":"Feature","id":0,"geometry":{"type":"Point","coordinates":[-8.1282480769999665,41.803837414000043,0]},"properties":{"FID":0,"Name":"Cascata do Rio Homem", "Descricao": "Esta é apenas uma das cascatas da Portela do Homem que se sucedem no rio Homem. É chamada de Cascata da Portela do Homem ou de S.Miguel e encontra-se inserido na freguesia de Campo do Gerês no concelhlo de Terras de Bouro. ","image":"https://turismo.cm-terrasdebouro.pt/wp-content/uploads/2020/01/cascata-portela-homem.jpg", "url" : "https://turismo.cm-terrasdebouro.pt/listings/cascata-da-portela-do-homem/"}},{"type":"Feature","id":1,"geometry":{"type":"Point","coordinates":[ -8.262891, 41.874453,0]},"properties":{"FID":1,"Name":"Soajo", "Descricao":"Soajo localiza-se no concelho de Arcos de Valdevez e é conhecido pela sua cultura, essencialmente os espigueiros e povoações. ", "image":"https://www.cmav.pt/thumbs/cmarcos/uploads/poi/image/213/ESPIGUEIROS_DE_SOAJO_1_1000_1000.JPG", "url":"https://www.cmav.pt/pages/1100?poi_id=213"}},{"type":"Feature","id":2,"geometry":{"type":"Point","coordinates":[-8.2310023999999657,41.653112900000053,0]},"properties":{"FID":2,"Name":"Barragem e Albufeira da Caniçada", "Descricao":"A barragem da Caniçada encontra-se na freguesia de Valdosende em Terras de Bouro, onde convergem os rios Cávado, Gerês e Freitas. Esta albufeira tem várias qualidades estéticas, atividades e pontos turísticos a oferecer, sendo estas a oferta de desportos náuticos, pontes e Marina de Rio Caldo, vista para o Santuário do São Bento da Porta Aberta e a Praia de Alquirão.","image":"https://turismo.cm-terrasdebouro.pt/wp-content/uploads/2019/05/ponte-rio-caldo-geres.jpg", "url":"https://turismo.cm-terrasdebouro.pt/listings/albufeira-da-canicada/"}}]}